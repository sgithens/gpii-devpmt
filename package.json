{
    "name": "gpii-devpmt",
    "productName": "devpmt",
    "version": "1.0.0",
    "description": "Developers Preference Testing Tool",
    "main": "devpmt.js",
    "scripts": {
        "start": "node devpmt.js",
        "watch-sass": "gulp",
        "build-sass": "gulp sass",
        "pretest": "node node_modules/rimraf/bin.js reports/* coverage/*",
        "test:vagrantSetup": "vagrant ssh -c 'git clone https://github.com/GPII/universal.git /home/vagrant/universal; cd /home/vagrant/universal; git checkout -b GPII-2966 origin/GPII-2966; npm install'",
        "test:vagrant": "vagrant ssh -c 'cd /home/vagrant/universal; npm start & cd /home/vagrant/sync/gpii-devpmt; sh ./scripts/testWhenReady.sh'",
        "test:node": "node node_modules/nyc/bin/nyc.js node tests/all-tests.js",
        "test": "npm run test:node",
        "posttest": "node node_modules/nyc/bin/nyc.js report -r text-summary -r html --report-dir reports --temp-directory coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gpii/gpii-devpmt"
    },
    "author": "Steven Githens <sgithens@raisingthefloor.org>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://issues.gpii.net/"
    },
    "homepage": "https://github.com/gpii/gpii-devpmt",
    "dependencies": {
        "connect-redis": "3.3.3",
        "express-session": "1.15.6",
        "file-saver": "1.3.8",
        "foundation-sites": "6.4.3",
        "infusion": "3.0.0-dev.20190328T144119Z.ec44dbfab",
        "gpii-handlebars": "1.1.5-dev.20190520T235701Z.1efa27b.GPII-3698",
        "gpii-universal": "0.3.0-dev.20190131T192812Z.7eb9cba7",
        "gpii-binder": "1.0.5",
        "markdown-it": "8.4.2",
        "gulp-autoprefixer": "3.1.1",
        "handlebars-helpers": "0.8.0",
        "json5": "1.0.1",
        "jsonpointer.js": "0.4.0",
        "lunr": "2.0.3"
    },
    "devDependencies": {
        "browserify": "16.2.3",
        "gpii-express": "1.0.15",
        "grunt": "1.0.3",
        "grunt-markdownlint": "2.1.0",
        "http-browserify": "1.7.0",
        "https-browserify": "1.0.0",
        "nyc": "13.1.0",
        "recursive-copy": "2.0.9",
        "testem": "2.10.0",
        "url": "0.11.0",
        "eslint": "3.14.1",
        "eslint-config-fluid": "1.1.0",
        "fluid-grunt-eslint": "18.1.2",
        "fluid-grunt-json5lint": "1.0.0",
        "gpii-webdriver": "1.0.2",
        "gpii-grunt-lint-all": "1.0.5",
        "grunt-shell": "1.3.0",
        "gulp": "3.9.1",
        "gulp-autoprefixer": "3.1.1",
        "gulp-load-plugins": "1.5.0",
        "gulp-sass": "2.3.2",
        "istanbul": "0.4.5",
        "kettle": "1.10.1",
        "node-jqunit": "1.1.8",
        "rimraf": "2.5.4",
        "shelljs": "0.3.0"
    }
}
